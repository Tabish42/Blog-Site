<%-include('partials/header')-%>
<h1>Home</h1>

<p><%=startingContent.slice(0,100)+"..."%></p>

<div>  
<%posts.forEach((post) => { %>
   
   <h2><%= post.title%></h2>
   <p><%= post.content.slice(0,100)+"..."%><a href="/compose/<%=post.title%>">Read More</a></p>
   
<% })%>
<a href="/compose"><button type="button" class="btn btn-primary">Compose New Post</button></a>
</div>



<%-include('partials/footer')-%>